   {% extends "partials/layout.njk" %}
   {% block title %}form{% endblock %}
   {% block content %}
    
    <h1>FORM ;p </h1>
    <form> 
        <p>
        <label for="fname"> First name <br></label>
        <input id="fname" type="text" placeholder="..." name="fname">
        </p>
        <p>
        <label for="lname"> Last name <br></label>
        <input id="lname" type="text" placeholder="..." name="lname">
        </p>
        <p>
        <label for="age"> Age <br></label>
        <input id="age" type="number" min="0" max="124" step="0.1" placeholder="..." name="age">
        </p>
        <input type="submit" value="Submit">
    
    </form>
   {% endblock %}